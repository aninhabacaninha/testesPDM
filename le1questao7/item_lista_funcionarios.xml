<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/item_lista_funcionarios_textView_nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Ana Clara"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        />

    <TextView
        android:id="@+id/item_lista_funcionarios_textView_sexo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Feminino"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_lista_funcionarios_textView_nome"
        />

    <TextView
        android:id="@+id/item_lista_funcionarios_textView_cargo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Arquiteta"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>